{
  "markdown-oxide": {
    "command": "markdown-oxide",
    "extensionToLanguage": {
      ".md": "markdown"
    }
  },
  "marksman": {
    "command": "marksman",
    "args": ["server"],
    "extensionToLanguage": {
      ".md": "markdown",
      ".mdx": "markdown.mdx"
    }
  },
  "terraform": {
    "command": "terraform-ls",
    "args": ["serve"],
    "extensionToLanguage": {
      ".tf": "terraform",
      ".tfvars": "terraform-vars"
    }
  }
}
