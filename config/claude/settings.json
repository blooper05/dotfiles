{
  "env": {
    "DISABLE_ERROR_REPORTING": 1,
    "DISABLE_TELEMETRY": 1
  },
  "includeCoAuthoredBy": false,
  "permissions": {
    "allow": [
      "Bash(cat:*)",
      "Bash(ls:*)",
      "Bash(mkdir:*)",
      "Bash(mv:*)",
      "Bash(test:*)",
      "Edit(*)",
      "Fetch(https://*)",
      "List(*)"
    ],
    "deny": []
  }
}
